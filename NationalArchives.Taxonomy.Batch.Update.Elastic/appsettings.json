{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "OpenSearchUpdateParams": {
    "BulkUpdateBatchSize": "1000",
    "QueueFetchSleepTime": "500"
  },
  "UpdateOpenSearchConnectionParameters": {
    "Scheme": "https",
    "Host": "vpc-",
    "Port": "443",
    "IndexDatabase": "discovery_records",
    "OpenSearchAwsParams": {
      "UseAwsConnection": "true",
      "Region": "eu-west-2",
      "RoleArn": "??",
      "AccessKey": "??",
      "SecretKey": "??"
    }
  },
  "UpdateStagingQueueParams": {
    "Uri": "*",
    "QueueName": "*",
    "MaxSize": "30000000",
    "UserName": "",
    "Password": ""
  }
}