{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AWS": {
    "Profile": "*",
    "ProfilesLocation": "*",
    "Region": "*"
  },
  "OpenSearchUpdateParams": {
    "BulkUpdateBatchSize": "1000",
    "QueueFetchSleepTime": "2000",
    "SearchDatabaseUpdateInterval": "1000",
    "MaxInternalQueueSize": "1000000"
  },
  "UpdateOpenSearchConnectionParameters": {
    "Scheme": "https",
    "Host": "vpc-",
    "Port": "443",
    "IndexDatabase": "discovery_records",
    "OpenSearchAwsParams": {
      "UseAwsConnection": "true",
      "Region": "eu-west-2",
      "RoleArn": "??",
      "AccessKey": "??",
      "SecretKey": "??"
    }
  },
  "AmazonSqsParams": {
    "QueueUrl": "*",
    "UseIntegratedSecurity": "true",
    "UseEC2Credentials": "false",
    "Region": "eu-west-2",
    "RoleArn": "*",
    "AccessKey": "*",
    "SecretKey": "*",
    "WaitMilliseconds": "20000",
    "Profile": "dev_sqs_user",
    "AssumeRole": "false"
  },
  "UpdateStagingQueueParams": {
    "Uri": "*",
    "QueueName": "*",
    "MaxSize": "30000000",
    "UserName": "",
    "Password": ""
  }
}