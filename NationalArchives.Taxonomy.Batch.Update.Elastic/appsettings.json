{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "AWS": {
    "Profile": "*",
    "ProfilesLocation": "*",
    "Region": "*"
  },
  "OpenSearchUpdateParams": {
    "BulkUpdateBatchSize": "1000",
    "QueueFetchSleepTime": "2000",
    "SearchDatabaseUpdateInterval": "1000",
    "MaxInternalQueueSize": "1000000",
    "WaitMilliseconds": "20000",
    "NullCounterHours": "1"
  },
  "DiscoveryOpenSearchParams": {
    "Scheme": "https",
    "Host": "vpc-",
    "Port": "443",
    "IndexDatabase": "discovery_records",
    "OpenSearchAwsParams": {
      "UseAwsConnection": "false",
      "OpenSearchConnectionMode": "Agnostic",
      "Region": "eu-west-2",
      "RoleArn": "",
      "AccessKey": "??",
      "SecretKey": "??"
    }
  },
  "UpdateStagingQueueParams":{
    "AmazonSqsParams": {
      "QueueUrl": "",
      "UseIntegratedSecurity": "true",
      "UseEC2Credentials": "true",
      "Region": "eu-west-2",
      "RoleArn": "",
      "AccessKey": "",
      "SecretKey": "",
      "WaitMilliseconds": "20000",
      "Profile": "dev_sqs_user",
      "AssumeRole": "false"
    }
  }
}