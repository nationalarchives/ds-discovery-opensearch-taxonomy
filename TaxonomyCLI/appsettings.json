{
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "DiscoveryOpenSearchParams": {
    "Scheme": "https",
    "Host": "vpc-",
    "Port": "443",
    "IndexDatabase": "discovery_records",
    "OpenSearchAwsParams": {
      "UseAwsConnection": "true",
      "Region": "eu-west-2",
      "RoleArn": "FROM_Env_Var",
      "AccessKey": "FROM_Env_Var",
      "SecretKey": "FROM_Env_Var"
    }
  },

  "CategoryOpenSearchParams": {
    "Scheme": "https",
    "Host": "vpc-",
    "Port": "443",
    "IndexDatabase": "categories_elastic"
  },
  "CategoryMongoParams": {
    "ConnectionString": "mongodb://",
    "DatabaseName": "taxonomy",
    "CollectionName": "categories_elastic2_new_fieldnames"
  },
  "CategorySource": "Mongo",

  "UpdateStagingQueueParams": {
    "Uri": "activemq:tcp://localhost:61616",
    "QueueName": "taxonomy.results1",
    "PostUpdates": "false"
  },
  "CategoriserLuceneParams": {
    "DefaultTaxonomyField": "textnocasnopunc",
    "QueryFields": [ "TITLE", "DESCRIPTION", "CONTEXT", "CATALOGUE_REFERENCE", "COVERING_DATES" ]
  }
}